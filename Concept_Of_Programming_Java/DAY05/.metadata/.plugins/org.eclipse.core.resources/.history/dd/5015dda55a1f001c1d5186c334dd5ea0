package sunbeam;

class Employee
{
	private int id;
	private String name;
	private int salary;
	Employee() // parameterless constructor 
	{
		
	}
	Employee(int id, String name, int salary) {
		super();
		this.id = id;
		this.name = name;
		this.salary = salary;
	}
	
	
	
}

